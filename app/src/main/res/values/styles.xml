<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme.noActionBar" parent="Theme.AppCompat.Light.NoActionBar"></style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar">

    </style>

    <style name="Button" />

    <style name="Button.Primary">
        <item name="android:textColor">#020202</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@drawable/button_background</item>
    </style>

    <style name="EditText" />

    <style name="EditText.Primary">
        <item name="android:textColor">#8D92A3</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="Tab">
        <item name="android:textColor">#8D92A3</item>
        <item name="android:textSize">14sp</item>
        <item name="textAllCaps">false</item>
        <item name="android:gravity">left</item>
